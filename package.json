{
  "name": "gawsddns",
  "version": "1.0.0",
  "description": "Dynamic DNS service using AWS CDK",
  "main": "lib/dd-stack.js",
  "scripts": {
    "build:java": "mvn clean package",
    "build:ts": "tsc",
    "build": "npm run build:java && npm run build:ts",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy",
    "diff": "npm run build && cdk diff",
    "synth": "npm run build && cdk synth"
  },
  "bin": {
    "dd": "bin/dd.js"
  },
  "keywords": [
    "aws",
    "cdk",
    "ddns",
    "route53"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-cdk-lib": "^2.212.0",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "aws-cdk": "^2.1025.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
